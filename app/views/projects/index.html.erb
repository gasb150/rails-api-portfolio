<p id="notice"><%= notice %></p>

<h1>Projects</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Framework</th>
      <th>Language</th>
      <th>Skill</th>
      <th>Other</th>
      <th>Imageurl</th>
      <th>Live url</th>
      <th>Source url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.name %></td>
        <td><%= project.framework_id %></td>
        <td><%= project.language_id %></td>
        <td><%= project.skill_id %></td>
        <td><%= project.other %></td>
        <td><%= project.imageurl %></td>
        <td><%= project.live_url %></td>
        <td><%= project.source_url %></td>
        <td><%= link_to 'Show', project %></td>
        <td><%= link_to 'Edit', edit_project_path(project) %></td>
        <td><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project', new_project_path %>
